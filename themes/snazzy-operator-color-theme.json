{
  "tokenColors": [
    {
      "name": "Global settings",
      "settings": {
        "background": "#282a36",
        "foreground": "#eff0eb"
      }
    },
    {
      "name": "Strong",
      "scope": "strong, markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Emphasis",
      "scope": "emphasis, markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Underline",
      "scope": "markup.underline",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Comment",
